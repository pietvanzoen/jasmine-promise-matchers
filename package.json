{
  "name": "@pietvanzoen/jasmine-promise-matchers",
  "version": "0.0.0-development",
  "description": "Adds toResolve and toReject matchers for jasmine.",
  "main": "index.js",
  "author": "hi@pietvanzoen.com",
  "scripts": {
    "test": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "test:watch": "watch 'npm test' ./lib ./test",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "node .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "condition-circle": "^1.5.0",
    "eslint": "^3.12.1",
    "git-hooks": "^1.1.7",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^3.1.0",
    "lodash": "^4.12.0",
    "rsvp": "^3.3.3",
    "semantic-release": "^7.0.1",
    "validate-commit-msg": "^2.8.2",
    "watch": "^1.0.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pietvanzoen/jasmine-promise-matchers.git"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "maxSubjectLength": 72
    }
  }
}
